generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Gender {
  MALE
  FEMALE
  OTHER
  PREFER_NOT_TO_SAY
}

enum StringBoolean {
  YES
  NO
}

enum ApplicationStatus {
  PENDING
  APPROVED
  REJECTED
}

enum JoinCommunityOption {
  YES
  NO
  ALREADY_MEMBER
}
